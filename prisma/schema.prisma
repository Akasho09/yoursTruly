generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Visitor {
  id        String     @id @default(cuid())
  ip        String     @unique
  city      String?
  region    String?
  country   String?
  latitude  Float?
  longitude Float?
  isp       String?
  visitedAt DateTime[] @default([])
}
